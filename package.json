    "@typescript-eslint/parser": "^8.4.0",
    "concurrently": "^9.2.0",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "iconify-icon": "^3.0.0"
